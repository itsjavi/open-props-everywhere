{
  "name": "open-props-dx",
  "version": "0.3.1",
  "description": "Extended version of open-props with extra tokens and better TypeScript support, compatible with PandaCSS tokens.",
  "keywords": [
    "open-props",
    "pandacss",
    "css",
    "css-in-js"
  ],
  "homepage": "https://github.com/itsjavi/open-props-dx",
  "bugs": "https://github.com/itsjavi/open-props-dx/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsjavi/open-props-dx.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && parcel build --no-cache && pnpm publint",
    "dev": "parcel watch",
    "prepublishOnly": "pnpm build",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "deepmerge": "^4.3.1"
  },
  "devDependencies": {
    "@pandacss/types": "^0.40.1",
    "@types/jest": "^29.5.12",
    "parcel": "^2.12.0",
    "publint": "^0.2.8",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
