{
  "name": "@itsjavi/open-props-everywhere",
  "version": "0.1.3",
  "type": "module",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "scripts": {
    "biome:fix": "biome check --max-diagnostics 50 --write .",
    "biome:lint": "biome lint . --max-diagnostics 50",
    "build": "parcel build",
    "format": "pnpm biome:fix && sort-package-json",
    "postinstall": "sort-package-json",
    "lint": "pnpm typecheck && pnpm biome:lint",
    "lint:fix": "pnpm format",
    "pub": "pnpm dlx jsr publish",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "ver": "npm version",
    "watch": "parcel watch"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@pandacss/types": "^0.40.1",
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@types/jest": "^29.5.12",
    "parcel": "^2.12.0",
    "sort-package-json": "^2.10.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  }
}
