{
  "name": "@itsjavi/open-props-everywhere",
  "version": "0.1.4",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "biome:fix": "biome check --max-diagnostics 50 --write .",
    "biome:lint": "biome lint . --max-diagnostics 50",
    "build": "rm -rf dist && parcel build --no-cache && pnpm publint",
    "prepublishOnly": "pnpm build",
    "format": "pnpm biome:fix && sort-package-json",
    "postinstall": "sort-package-json",
    "lint": "typelint && pnpm typecheck && pnpm biome:lint",
    "lint:fix": "pnpm format",
    "pub": "echo 'Not implemented yet'",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "ver": "npm version",
    "watch": "parcel watch"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.0",
    "@pandacss/types": "^0.40.1",
    "@parcel/packager-ts": "2.12.0",
    "@parcel/transformer-typescript-types": "2.12.0",
    "@types/jest": "^29.5.12",
    "parcel": "^2.12.0",
    "publint": "^0.2.8",
    "sort-package-json": "^2.10.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
